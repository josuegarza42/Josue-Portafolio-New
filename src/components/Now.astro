---
import Card from "./Card/index.astro";
import Pulse from "./Pulse.astro";
---

<Card colSpan="md:col-span-1" rowSpan="md:row-span-2">
  <div class="flex justify-between w-full items-start mb-2">
    <div class="flex flex-col">
      <ul class="list-disc list-inside">
        <span class="text-xs text-gray-500 cursor-pointer">
          <a
            href="https://www.qualtrics.com/"
            target="_blank"
            rel="noopener noreferrer"
            class="text-red-500 hover:text-red-700 transition-colors">Zendesk</a
          >
        </span>
        <li>Associate Services Consultant</li>
      </ul>
    </div>
    <Pulse />
  </div>
  <!--

     -->
</Card>
